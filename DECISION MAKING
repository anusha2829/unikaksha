if s>=60.00:
    print("yes")
else:
    print("No")
    
#Budget
A=int(input())
B=int(input())
C=int(input())
D=int(input())
E=int(input())
a=A*350.34
b=B*230.90
c=C*190.55
d=D*125.30
e=E*180.90
sum=a+b+c+d+e
if sum<=15000:
    print("Yes")
else:
    print("No")
    
#Sece Dining
a=input()
b=int(input())
if a=="front" and b==1 or a=="rear" and b==2:
    print("Left Handed")
elif a=="rear" and b==1 or a=="front" and b==2:
    print("Right Handed")
else:
    print("Invalid input")
    
#Intersection of a Circle
x1=int(input())
y1=int(input())
r1=int(input())
x2=int(input())
y2=int(input())
r2=int(input())
r=r1+r2
d=((((x1-x2)**2)+((y1-y2)**2))**0.5)
if d==r:
    print("The circles are tangential")
elif d<r:
    print("The circles  overlap")
else:
    print("The circles do not overlap")
    
#Microwave Oven
print("Enter the number of items")
n=int(input())
print("Enter the single item heating time")
ht=float(input())
x=ht*(50/100)
y=ht+x
z=2*ht
if n==2:
    print("The recommended heating time is %.2f"%y)
elif n==3:
    print("The recommended heating time is %.2f"%z)
elif n>3:
    print("Number of items is more")
else:
    print("Invalid input")
